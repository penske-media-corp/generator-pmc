WP_CORE_VERSION={{getv "/pmc/wp/core/version" "<%= wpVersion %>"}}
WP_DB_HOST={{getv "/pmc/wp/db/host" "<%= wpDbHost %>"}}
WP_DB_NAME={{getv "/pmc/wp/db/name" "<%= wpDbName %>"}}
WP_DB_PASS={{getv "/pmc/wp/db/pass" "<%= wpDbPass %>"}}
WP_DB_USER={{getv "/pmc/wp/db/user" "<%= wpDbUser %>"}}
WP_DOMAIN={{getv "/pmc/wp/domain" "<%= wpDomain %>"}}
WP_EMAIL={{getv "/pmc/wp/email" "<%= wpEmail %>"}}
WP_ENV={{getv "/pmc/wp/env" "true"}}
WP_THEME={{getv "/pmc/wp/theme" "<%= wpTheme %>"}}
WP_TITLE={{getv "/pmc/wp/title" "<%= wpTitle %>"}}
