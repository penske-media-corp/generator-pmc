PMC_PHPLOC={{getv "/pmc/pmc/phploc" "false"}}
PMC_PHPMD={{getv "/pmc/pmc/phpmd/" "false"}}
PMC_SHARED_CODE_PATH={{getv "/pmc/pmc/shared/code/path" "./vendor/pmc" }}
PMC_TEXT_DOMAIN={{getv "/pmc/pmc/text/domain" "<%= textDomain %>"}}
