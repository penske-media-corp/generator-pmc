WP_TESTS_DB_PASS={{getv "/pmc/wp/tests/db/pass" "wptests"}}
WP_TESTS_DB_USER={{getv "/pmc/wp/tests/db/user" "wptests"}}
WP_TESTS_DIR={{getv "/pmc/wp/tests/dir" "<%= wpDir %>/wp-tests/tests/phpunit/"}}
