PMC_DIFF_ONLY={{getv "/pmc/pmc/diff/only" "false"}}
PMC_ESLINT={{getv "/pmc/pmc/eslint" "true" }}
PMC_LOCALIZATION={{getv "/pmc/pmc/localization" "true"}}
PMC_PHPCBF={{getv "/pmc/pmc/phpcbf" "false"}}
PMC_PHPCPD={{getv "/pmc/pmc/phpcpd" "false"}}
PMC_PHPCS={{getv "/pmc/pmc/phpcs" "true"}}
PMC_PHPCS_STANDARD={{getv "/pmc/pmc/phpcs/standard" "PmcWpVip"}}
