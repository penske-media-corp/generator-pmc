<% if (wpMultisite) { %>MULTISITE={{getv "/pmc/multisite" "true"}}<% } %>
GID={{getv "/pmc/gid" "100"}}
JETPACK_DEV_DEBUG={{getv "/pmc/jetpack/dev/debug" "true"}}
MYSQL_ALLOW_EMPTY_PASSWORD={{getv "/pmc/mysql/allow/empty/password" "yes"}}
MYSQL_DATABASE={{getv "/pmc/mysql/database" "<%= wpDbName %>"}}
MYSQL_ROOT_PASSWORD={{getv "/pmc/mysql/root/password" ""}}
<% if (wpHasBuild) { %>
PMC_BUILD_COMMAND={{getv "/pmc/pmc/build/command" "<%= buildCommand %>"}}
